    "twitter": "1.7.1",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/node": "14.11.2",
    "@types/twitter": "1.7.0",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "eslint": "7.10.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
